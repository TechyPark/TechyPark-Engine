{
  "name": "techypark-engine",
  "version": "1.0.0",
  "description": "The main package and workspace manager for the TechyPark Engine project.",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "build": "npm run build --workspace=frontend && npm run build --workspace=backend",
    "start": "npm start --workspace=backend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechyPark/TechyPark-Engine.git"
  },
  "keywords": [
    "hono",
    "react",
    "engine",
    "web"
  ],
  "author": "TechyPark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TechyPark/TechyPark-Engine/issues"
  },
  "homepage": "https://github.com/TechyPark/TechyPark-Engine#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.5.3"
  }
}

